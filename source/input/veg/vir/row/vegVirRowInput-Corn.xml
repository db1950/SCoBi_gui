<!-- row_space_m - The distance without vegetation between two rows (m)  -->  
<!-- col_space_m - The distance between two adjacent plants within a row (m)-->
<!-- phi_row_deg - Azimuth angle of field rows from local North (degrees) -->
<!-- plant_row_spread_m - Max scattering dist. of a plant pos between rows (m)  -->
<!-- plant_col_spread_m - Max scattering dist. of a plant pos within a row (m) -->


<plugin>corn_20171107</plugin>

<row_space_m>0.75</row_space_m>
<col_space_m>0.20</col_space_m>

<phi_row_deg>0</phi_row_deg>

<plant_row_spread_m>3e-2</plant_row_spread_m>
<plant_col_spread_m>2e-2</plant_col_spread_m>

</VEGETATION_VIRTUAL_ROW_INPUT>
